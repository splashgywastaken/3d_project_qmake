    RESOURCES = $$PWD/shaders.qrc

    DISTFILES += \
    $$PWD/basicShader/basicShaders.vert \
    $$PWD/basicShader/basicShaders.frag \
    $$PWD/lightningShader/lightningShader.vert \
    $$PWD/lightningShader/lightningShader.frag \
    $$PWD/normalShaders/normalShaders.vert \
    $$PWD/normalShaders/normalShaders.frag
}
